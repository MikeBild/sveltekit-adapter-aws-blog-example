{
  "name": "blog",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "AWS_SDK_LOAD_CONFIG=1 vite dev",
    "build": "npm run slides && vite build",
    "preview": "vite preview",
    "test": "AWS_SDK_LOAD_CONFIG=1 playwright test",    
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check .",
    "format": "prettier --plugin-search-dir . --write .",
    "destroy": "destroy",
    "slides": "marp SLIDES.md -o static/slides.html"
  },
  "devDependencies": {
    "dotenv": "16.0.2",
    "@marp-team/marp-cli": "2.2.0",
    "@picocss/pico": "1.5.5",
    "@playwright/test": "1.26.1",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "aws-sdk": "2.1225.0",
    "mdsvex": "0.10.6",
    "playwright": "1.26.1",
    "prettier": "2.7.1",
    "prettier-plugin-svelte": "2.7.1",
    "sass": "1.55.0",
    "svelte": "3.50.1",
    "svelte-check": "2.9.1",
    "svelte-preprocess": "4.10.7",
    "sveltekit-adapter-aws": "4.3.1",
    "tslib": "2.4.0",
    "typescript": "4.8.4",
    "vite": "3.1.3"
  },
  "type": "module"
}
